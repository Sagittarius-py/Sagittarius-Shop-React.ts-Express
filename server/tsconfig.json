{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES5",
    "sourceMap": true,
    "jsx": "preserve",
    "allowJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "moduleResolution": "NodeNext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "exclude": [
    "client",
    "dist",
    "node_modules"
  ]
}